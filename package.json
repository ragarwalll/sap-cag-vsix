{
  "name": "create-sap-fs-app-vsix",
  "displayName": "SAP Cloud Application Generator",
  "description": "Creating SAP full stack applications in seconds.",
  "version": "1.2.0",
  "publisher": "ragarwal06",
  "license": "ISC",
  "repository": {
    "url":  "https://github.com/ragarwalll/sap-cloud-application-generator-vsix.git",
    "type": "git"
  },
  "icon": "assets/logo.png",
  "categories": [
    "Programming Languages",
    "Extension Packs",
    "Other"
  ],
  "keywords": [
    "vs-code",
    "extension",
    "vs-code-extension",
    "create-sap-app",
    "create-sap-cloud-application",
    "ui5",
    "spring-boot",
    "nodejs",
    "reactjs",
    "cf"
  ],
  "engines": {
    "node": ">=14.19",
    "vscode": "^1.46.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sap-cloud-application-generator-vsix.open-generator",
        "title": "SAP Cloud Applications: Generate"
      }
    ]
  },
  "scripts": {
    "build": "npm run build:webview && npm run compile",
    "start:webview": "cd webview-ui && npm run dev",
    "build:webview": "cd webview-ui && npm run build",
    "test:webview": "cd webview-ui && npm run test",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "null-loader": "^4.0.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@ragarwal06/sap-cloud-application-generator": "^1.1.8",
    "@ragarwal06/sap-cloud-application-generator-types": "^0.1.3"
  },
  "bundledDependencies": [
    "@ragarwal06/sap-cloud-application-generator",
    "@ragarwal06/sap-cloud-application-generator-types"
  ]
}
